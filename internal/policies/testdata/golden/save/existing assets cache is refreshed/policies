gpos:
    - id: '{GPOId}'
      name: GPOName
      rules:
        dconf:
            - key: path/to/key1
              value: ValueOfKey1
              disabled: false
              meta: s
            - key: path/to/key2
              value: |
                ValueOfKey2
                On
                Multilines
              disabled: false
              meta: s
        scripts:
            - key: path/to/key3
              value: |
                ValueOfKey3
                On
                Multilines
              disabled: false
              strategy: append
